program lab1;

    var
        a,b,x,z:real;
    begin
        writeLn('Введите A: ');
        readLn(A);
        writeLn('Введите B: ');
        readLn(B);
        
        x:=sqrt(abs(a) + sqr(a+b) / a * b);
        
        z:= b * (sin(x)/cos(x) + exp(x));
        
        
        writeLn('Вывод результатов:');
        writeLn('z=', z:0:4);
    end.
    

